<pattern><w pos="N"/><w pos="AJ"/></pattern> <!-- ηλίου φαεινότερο, χρόνια πολλά-->
<pattern><w pos="N"/><w pos="N"/></pattern> <!-- κράτος μέλος, υπουργείο εσωτερικών-->
<pattern><w pos="N"/><w pos="DT"/><w pos="N"/></pattern> <!-- φαινόμενο του θερμοκηπίου-->
<pattern><w pos="N"/><w pos="AJ"/><w pos="N"/></pattern> <!-- τεχνολογία υψηλής ευκρίνειας-->
<pattern><w pos="N"/><w pos="P"/><w pos="N"/></pattern> <!-- φόνος εκ προμελέτης-->
<pattern><w pos="V"/><w pos="N"/></pattern> <!-- βρέχει καρέκλες -->
