<pattern><w pos="V"/><w pos="P"/><w pos="N"/></pattern> <!-- θέτω σε κίνδυνο -->
<pattern><w pos="P"/><w pos="N"/></pattern> <!-- επί τάπητος -->
<pattern><w pos="P"/><w pos="AJ"/></pattern> <!-- εν ολίγοις -->
<pattern><w pos="P"/><w pos="V"/></pattern> <!-- πέρα βρέχει -->
<pattern><w pos="P"/><w pos="DT"/><w pos="AJ"/></pattern> <!-- με το καλό -->
<pattern><w pos="P"/><w pos="N"/><w pos="N"/></pattern> <!-- κατά φαντασίαν ασθενής -->
<pattern><w pos="P"/><w pos="AJ"/><w pos="N"/></pattern> <!-- εν πάση περιπτώσει -->
<pattern><w pos="ADV"/><w pos="V"/></pattern> <!-- καλώς όρισες -->
